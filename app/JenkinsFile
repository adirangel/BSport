pipeline {
    agent any

    stages {
        stage ('Compile Stage'){
            steps{
                    sh 'mvm clean'
                 }
        }
        stage ('Testing Stage'){
            steps{
                    sh 'mvm test'
                 }
        }
        stage ('package Stage'){
            steps{
                        sh 'mvm package'
                }
        }

    }
}
